    "@nativescript/tailwind": "^4.0.5",
    "@nativescript/vite": "^1.0.2",
    "@valor/nativescript-websockets": "^2.0.2",
    "dominative": "^0.1.3",
    "hono": "^4.10.5",
    "nativescript-inappbrowser": "^3.2.0",
    "solid-js": "^1.8.21",
    "solid-navigation": "/Users/carlo/Desktop/Projects/solid-navigation",
    "tailwindcss": "^4.1.17",
    "undom-ng": "^1.1.2",
    "url": "^0.11.4",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.28.5",
    "@biomejs/biome": "^2.3.5",
    "@nativescript-dom/core-types": "1.0.32",
    "@nativescript-dom/solidjs-types": "1.0.25",
    "@nativescript/ios": "~9.0.0",
    "@nativescript/types": "~9.0.0",
    "@nativescript/webpack": "~5.0.0",
    "babel": "^6.23.0",
    "babel-loader": "^10.0.0",
    "babel-preset-solid": "^1.8.19",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "plist": "^3.1.0",
    "solid-refresh": "^0.7.5",
    "typescript": "~5.9.3",
    "wait-on": "^9.0.3"
  },
  "scripts": {
    "dev": "ns run ios",
    "podinstall": "cd platforms/ios && pod install",
    "apigen": "curl -s http://localhost:3000/api/docs/hono > src/lib/api.d.ts",
    "check": "tsc --noemit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "dev:ios": "concurrently -k -n vite,ns 'npm run dev:server:ios' 'wait-on tcp:5173 && npm run ios'",
    "dev:android": "concurrently -k -n vite,ns 'npm run dev:server:android' 'wait-on tcp:5173 && npm run android'",
    "dev:server:ios": "VITE_DEBUG_LOGS=1 vite serve -- --env.ios --env.hmr",
    "dev:server:android": "VITE_DEBUG_LOGS=1 vite serve -- --env.android --env.hmr",
    "ios": "VITE_DEBUG_LOGS=1 ns debug ios",
    "android": "VITE_DEBUG_LOGS=1 ns debug android"
  }
}
